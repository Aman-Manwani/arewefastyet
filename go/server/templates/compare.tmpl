<!DOCTYPE html>
<html lang="en">

{{template "headHTML" .}}

<body>

    <!-- Navigation -->
    {{ template "navigation" "/compare" }}

    <section class="py-5">
        <div class="container">
            <h1>Compare</h1>

            <div class="main">
                <form type="GET" action="/compare">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Reference commit SHA" name="r">
                        <input type="text" class="form-control" placeholder="Compare commit SHA" name="c">
                        <div class="input-group-append">
                            <button class="btn btn-secondary" type="submit">
                                Compare
                            </button>
                        </div>
                    </div>
                </form>
            </div>

            {{ if .compare }}
                <h2>Comparing <a href="https://github.com/vitessio/vitess/commit/{{ .compare.SHA }}" target="_blank">{{.compare.short}}</a> with <a href="https://github.com/vitessio/vitess/commit/{{ .reference.SHA }}" target="_blank">{{.reference.short}}</a>.</h2>
                {{ $compareSHAs := .compare }}
                {{ $referenceSHAs := .reference }}

                {{ if .macrobenchmark }}
                    {{ range $key, $val := .macrobenchmark }}
                    <h4>{{ $key.ToUpper }}</h4>
                    <table class="table table-striped table-hover table-sm table-bordered">
                        <thead>
                        <tr>
                            <th scope="col"></th>
                            <th scope="col" class="text-center">QPS Total</th>
                            <th scope="col" class="text-center">QPS Reads</th>
                            <th scope="col" class="text-center">QPS Writes</th>
                            <th scope="col" class="text-center">QPS Other</th>
                            <th scope="col" class="text-center">TPS</th>
                            <th scope="col" class="text-center">Latency</th>
                            <th scope="col" class="text-center">Errors</th>
                            <th scope="col" class="text-center">Reconnects</th>
                            <th scope="col" class="text-center">Time</th>
                            <th scope="col" class="text-center">Threads</th>
                        </tr>
                        </thead>
                        <tbody>
                        {{ range $elem := $val }}
                        <tr>
                            <th scope="row" class="text-center"><a href="https://github.com/vitessio/vitess/commit/{{ $compareSHAs.SHA }}" target="_blank">{{$compareSHAs.short}}</a></th>
                            <td class="text-center">{{ $elem.Compare.QPS.TotalStr }}</td>
                            <td class="text-center">{{ $elem.Compare.QPS.ReadsStr }}</td>
                            <td class="text-center">{{ $elem.Compare.QPS.WritesStr }}</td>
                            <td class="text-center">{{ $elem.Compare.QPS.OtherStr }}</td>
                            <td class="text-center">{{ $elem.Compare.TPSStr }}</td>
                            <td class="text-center">{{ $elem.Compare.LatencyStr }}</td>
                            <td class="text-center">{{ $elem.Compare.ErrorsStr }}</td>
                            <td class="text-center">{{ $elem.Compare.ReconnectsStr }}</td>
                            <td class="text-center">{{ $elem.Compare.TimeStr }}</td>
                            <td class="text-center">{{ $elem.Compare.ThreadsStr }}</td>
                        </tr>
                        <tr>
                            <th scope="row" class="text-center"><a href="https://github.com/vitessio/vitess/commit/{{ $referenceSHAs.SHA }}" target="_blank">{{$referenceSHAs.short}}</a></th>
                            <td class="text-center">{{ $elem.Reference.QPS.TotalStr }}</td>
                            <td class="text-center">{{ $elem.Reference.QPS.ReadsStr }}</td>
                            <td class="text-center">{{ $elem.Reference.QPS.WritesStr }}</td>
                            <td class="text-center">{{ $elem.Reference.QPS.OtherStr }}</td>
                            <td class="text-center">{{ $elem.Reference.TPSStr }}</td>
                            <td class="text-center">{{ $elem.Reference.LatencyStr }}</td>
                            <td class="text-center">{{ $elem.Reference.ErrorsStr }}</td>
                            <td class="text-center">{{ $elem.Reference.ReconnectsStr }}</td>
                            <td class="text-center">{{ $elem.Reference.TimeStr }}</td>
                            <td class="text-center">{{ $elem.Reference.ThreadsStr }}</td>
                        </tr>
                        <tr>
                            <th scope="row" class="text-center">%</th>
                            <td class="text-center">{{ $elem.Diff.QPS.TotalStr }}</td>
                            <td class="text-center">{{ $elem.Diff.QPS.ReadsStr }}</td>
                            <td class="text-center">{{ $elem.Diff.QPS.WritesStr }}</td>
                            <td class="text-center">{{ $elem.Diff.QPS.OtherStr }}</td>
                            <td class="text-center">{{ $elem.Diff.TPSStr }}</td>
                            <td class="text-center">{{ $elem.Diff.LatencyStr }}</td>
                            <td class="text-center">{{ $elem.Diff.ErrorsStr }}</td>
                            <td class="text-center">{{ $elem.Diff.ReconnectsStr }}</td>
                            <td class="text-center">{{ $elem.Diff.TimeStr }}</td>
                            <td class="text-center">{{ $elem.Diff.ThreadsStr }}</td>
                        </tr>
                        {{ end }}
                        </tbody>
                    </table>
                    {{ end }}
                {{ else if not .macrobenchmark }}

                {{ end }}

                {{ if .microbenchmark }}
                <h3>Micro benchmarks</h3>

                <table class="table table-striped table-hover table-sm table-bordered">
                    <thead>
                        <tr>
                            <th scope="col">Package</th>
                            <th scope="col">Benchmark Name</th>
                            <th scope="col" colspan="2" class="text-center">number of iterations</th>
                            <th scope="col" colspan="1" class="text-center">Diff</th>
                            <th scope="col" colspan="2" class="text-center">nanosecond/op</th>
                            <th scope="col" colspan="2" class="text-center">B/op</th>
                            <th scope="col" colspan="2" class="text-center">MB/s</th>
                            <th scope="col" colspan="2" class="text-center">Allocs/op</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{range $val := .microbenchmark}}
                        <tr>
                            <td>{{ $val.PkgName }}</td>
                            <td><a href="https://github.com/vitessio/vitess/search?q={{ $val.Name }}&type=code" target="_blank">{{ $val.Name }}</a></td>
                            <td class="text-right">{{ $val.Current.OpsStr }}</td>
                            <td class="text-right">{{ $val.Last.OpsStr }}</td>

                            <td class="text-right {{if le $val.CurrLastDiff 0.90 }} bg-success {{ else if ge $val.CurrLastDiff 1.10 }} bg-danger {{ end }}">{{ $val.CurrLastDiffStr }}</td>
                            <td class="text-right">{{ $val.Current.NSPerOpStr }}</td>
                            <td class="text-right">{{ $val.Last.NSPerOpStr }}</td>

                            <td class="text-right">{{ $val.Current.MBPerSecStr }}</td>
                            <td class="text-right">{{ $val.Last.MBPerSecStr }}</td>

                            <td class="text-right">{{ $val.Current.BytesPerOpStr }}</td>
                            <td class="text-right">{{ $val.Last.BytesPerOpStr }}</td>

                            <td class="text-right">{{ $val.Current.AllocsPerOpStr }}</td>
                            <td class="text-right">{{ $val.Last.AllocsPerOpStr }}</td>
                        </tr>
                        {{end}}
                    </tbody>
                </table>
                {{ end }}
            {{ end }}
        </div>
    </section>

    <!------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------>


  {{template "footerHTML" .}}

</body>

</html>
